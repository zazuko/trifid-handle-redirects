@prefix http:   <http://www.w3.org/2011/http#>.
@prefix ex:     <http://localhost:3000/> .

ex:alice
    a       ex:Person ;
    ex:name "Alice" .

ex:req
    a               http:GetRequest ;
    http:response   [ a                             http:Response ;
                      <http://schema.org/validFrom> "2022-02-31T00:00:00Z"^^<http://www.w3.org/2001/XMLSchema#dateTime> ;
                      http:responseCode             http:301 ;
                      http:location                 ex:bob ] ;
    http:requestURI ex:req .


ex:bob
    a       ex:Person ;
    ex:name "Bob" .
